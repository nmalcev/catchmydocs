{
    "env": {
        "browser": true,
        "jest": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 1,
        "no-extra-parens": 1,
        "no-unreachable-loop": 1,
        "no-useless-backreference": 1,
        "block-scoped-var": 1,
        "class-methods-use-this": 1,
        "default-case": 1,
        "default-case-last": 1,
        "dot-location": 1,
        "dot-notation": 1,
        "eqeqeq": 1,
        "no-alert": 1,
        "no-caller": 1,
        "no-eval": 1,
        "no-implied-eval": 1,
        "no-invalid-this": 1,
        "no-implicit-globals": 1
    }
}
